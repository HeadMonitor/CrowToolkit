<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:CrowEngineProjectManager.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="CrowEngineProjectManager.MainWindow"
        Title="CrowEngineProjectManager"
        Background="#303030">
    
        <Window.Styles>
                <StyleInclude Source="../Styles/TabStyle.axaml"/>
        </Window.Styles>
    
        <TabControl>
                <TabItem Header="Project Manager">
                        <Grid ColumnDefinitions="*, 120">
                                <views:ProjectList Grid.Column="0"/>
                                <views:ProjectManagerButtons Grid.Column="1"/>
                        </Grid>
                </TabItem>
                <TabItem Header="dwdw"></TabItem>
        </TabControl>
</Window>
