<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="CrowEngineProjectManager.Views.ProjectManagerButtons">
    
    <Grid HorizontalAlignment="Right">
        <StackPanel VerticalAlignment="Top">
            <Button Margin="5" Width="100" Foreground="#FFFFFF">
                <Button.Content>
                    <TextBlock Text="New" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                </Button.Content>
            </Button>
            
            <Button Margin="5" Width="100" Foreground="#FFFFFF">
                <Button.Content>
                    <TextBlock Text="Import" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                </Button.Content>
            </Button>
            
            <Separator/>
        </StackPanel>
    </Grid>
</UserControl>
